{
  "name": "nasa-frontend",
  "version": "1.0.0",
  "description": "A simple front to the nasa epic API",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "build": "vite build && node scripts/postbuild.js",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "serve": "vite serve"
  },
  "keywords": [
    "showcase",
    "svelte",
    "pagecrypt"
  ],
  "author": "Lassi Piironen <lsipii@kapsi.fi>",
  "license": "MIT",
  "devDependencies": {
    "@lsipii/transformation-helpers": "^1.1.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "@tsconfig/svelte": "^3.0.0",
    "date-picker-svelte": "^2.0.0",
    "dotenv": "^16.0.1",
    "fs-extra": "^10.1.0",
    "pagecrypt": "^5.4.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vite-plugin-singlefile": "^0.10.0-beta.2"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all",
    "svelteSortOrder": "scripts-options-markup-styles",
    "svelteStrictMode": false,
    "svelteBracketNewLine": true,
    "svelteAllowShorthand": true,
    "plugins": [
      "./node_modules/prettier-plugin-svelte"
    ]
  },
  "engines": {
    "node": ">= 15.0.0"
  },
  "type": "module"
}